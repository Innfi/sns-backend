{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs", 
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "dist",
    "strictNullChecks": true,
    "sourceMap": true,
    "typeRoots": ["node_modules/@types"],
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
}